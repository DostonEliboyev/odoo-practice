<odoo>
  <data>
      <record id="view_uicecommerce_orderline_list" model="ir.ui.view">
        <field name="name">uicecommerce.orderline.list</field>
        <field name="model">uicecommerce.orderline</field>
        <field name="arch" type="xml">
            <list>
                <field name="product"/>
                <field name="value"/>
                <field name="salesprice"/>
                <field name="tax"/>
                <field name="discount"/>
                <field name="totalprice"/>
                <field name="profitpercent"/>
                <field name="profitmoney"/>
            </list>
        </field>
    </record>

    <record id="view_uicecommerce_orderline_form" model="ir.ui.view">
        <field name="name">uicecommerce.orderline.form</field>
        <field name="model">uicecommerce.orderline</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="product"/>
                        <field name="value"/>
                        <field name="salesprice"/>
                        <field name="tax"/>
                        <field name="discount"/>
                        <field name="totalprice"/>
                        <field name="profitpercent"/>
                        <field name="profitmoney"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_uicecommerce_orderline" model="ir.actions.act_window">
        <field name="name">Order Lines</field>
        <field name="res_model">uicecommerce.orderline</field>
        <field name="view_mode">list,form</field>
    </record>


  </data>
</odoo>
